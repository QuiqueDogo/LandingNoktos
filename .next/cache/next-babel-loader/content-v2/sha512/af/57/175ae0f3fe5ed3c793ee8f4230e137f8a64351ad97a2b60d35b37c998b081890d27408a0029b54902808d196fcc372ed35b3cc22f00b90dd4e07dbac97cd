{"ast":null,"code":"import { jsx as _jsx } from \"react/jsx-runtime\";\nimport { useRouter } from \"next/router\";\nimport Button from \"@mui/material/Button\";\nimport { useState, useEffect } from \"react\";\n\nconst NavItem = ({\n  title,\n  link,\n  setIsOpen\n}) => {\n  const router = useRouter();\n  const {\n    0: isActive,\n    1: setIsActive\n  } = useState(false);\n  useEffect(() => {\n    const {\n      pathname\n    } = router;\n    if (link === pathname) setIsActive(true);\n  });\n  return /*#__PURE__*/_jsx(Button, {\n    style: {\n      padding: \"16px 20px\",\n      color: !isActive ? \"#979797\" : \"#00c2ff\",\n      textTransform: \"none\",\n      fontSize: 15.5,\n      fontWeight: 600,\n      fontFamily: \"DM Sans Regular\"\n    },\n    onClick: () => {\n      if (setIsOpen) setIsOpen(false);\n      router.push(link);\n    },\n    children: title\n  });\n};\n\nNavItem.defaultProps = {\n  isActive: false,\n  setIsOpen: null\n};\nexport default NavItem;","map":null,"metadata":{},"sourceType":"module"}